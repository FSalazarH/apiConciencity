{
  "name": "Resident",
  "plural": "Residents",
  "base": "user",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "active": {
        "type": "Boolean",
        "required": false, 
        "default": false
      }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "bucket": {
      "type": "hasOne",
      "model": "Bucket",
      "foreignKey": ""
    },
    "wasteCollections": {
      "type": "hasMany",
      "model": "WasteCollection",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login",
        "create"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "authenticated",
      "permission": "ALLOW",
      "property": [
        "logout",
        "changePassword"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "patchAttributes",
        "__get__community",
        "__get__bucket",
        "__get__lastFourWasteCollection",
        "__get__dateCollection"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "find",
        "deleteById",
        "patchAttributes"
      ]
    }
  ],
  "methods": {}
}
