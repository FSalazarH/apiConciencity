{
  "name": "Resident",
  "plural": "Residents",
  "base": "user",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "active": {
      "type": "Boolean",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "bucket": {
      "type": "hasOne",
      "model": "Bucket",
      "foreignKey": ""
    },
    "assignments": {
      "type": "hasMany",
      "model": "Assignment",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "recolectionData": {
      "type": "hasMany",
      "model": "RecolectionData",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login",
        "create"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "authenticated",
      "permission": "ALLOW",
      "property": [
        "logout",
        "changePassword",
        "__create__assignments"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "patchAttributes",
        "customDelete",
        "__get__assignments",
        "__get__recolectionData"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "find",
        "patchAttributes",
        "__updateById__assignments",
        "__get__bucket"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "recycler",
      "permission": "ALLOW",
      "property": [
        "__post__data"
      ]
    }
  ],
  "methods": {}
}
