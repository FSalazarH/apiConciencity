{
  "name": "Recycler",
  "base": "user",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "active": {
      "type": "Boolean",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "scale": {
      "type": "hasOne",
      "model": "Scale",
      "foreignKey": ""
    },
    "notifications": {
      "type": "hasMany",
      "model": "Notification",
      "foreignKey": ""
    },
    "assignments": {
      "type": "hasMany",
      "model": "Assignment",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "recolectionData": {
      "type": "hasMany",
      "model": "RecolectionData",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login",
        "create",
        "changePassword"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "authenticated",
      "permission": "ALLOW",
      "property": [
        "logout",
        "findById",
        "__get__scale",
        "__get__notifications",
        "__create__assignments",
        "__get__assignments"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "patchAttributes",
        "__get__recyclerCommunities",
        "__get__community__composter_measurementsSensor",
        "__updateById__notifications",
        "customDelete"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "find",
        "deleteById",
        "patchAttributes",
        "__updateById__assignments"
      ]
    }
  ],
  "methods": {}
}
