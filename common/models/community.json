{
  "name": "Community",
  "base": "CustomPersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "floorsQuantity": {
      "type": "number"
    },
    "image": {
      "type": "string"
    },
    "dateCollection": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "commune": {
      "type": "belongsTo",
      "model": "Commune",
      "foreignKey": ""
    },
    "managers": {
      "type": "hasMany",
      "model": "Manager",
      "foreignKey": ""
    },
    "residences": {
      "type": "hasMany",
      "model": "Residence",
      "foreignKey": ""
    },
    "recyclers": {
      "type": "hasMany",
      "model": "RecyclerCommunity",
      "foreignKey": ""
    },
    "composter": {
      "type": "embedsOne",
      "model": "Composter",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "residenceAdmin",
      "permission": "ALLOW",
      "property": [
        "findById",
        "getSomething"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "updateAttributes",
        "__get__composters",
        "__create__composters",
        "create"
      ]
    }
  ],
  "methods": {}
}
