{
  "name": "Community",
  "base": "CustomPersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "floorsQuantity": {
      "type": "number"
    },
    "image": {
      "type": "string"
    },
    "dateCollection": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "commune": {
      "type": "belongsTo",
      "model": "Commune",
      "foreignKey": ""
    },
    "managers": {
      "type": "hasMany",
      "model": "Manager",
      "foreignKey": ""
    },
    "residences": {
      "type": "hasMany",
      "model": "Residence",
      "foreignKey": ""
    },
    "recyclers": {
      "type": "hasMany",
      "model": "RecyclerCommunity",
      "foreignKey": ""
    },
    "composter": {
      "type": "hasOne",
      "model": "Composter",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "find",
        "patchAttributes",
        "deleteById",
        "__get__residences__wasteCollections",
        "__get__composters",
        "__create__residences",
        "__create__composters",
        "__get__residences",
        "__get__managers",
        "__get__recyclers"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "manager",
      "permission": "ALLOW",
      "property": [
        "__get__residences"
      ]
    }
  ],
  "methods": {}
}
