{
  "name": "user",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": false
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": {
        "country": "string",
        "city":"string",
        "commune": "string",
        "street": "string",
        "location": "array"
      }
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "realm": false
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
