{
  "name": "Residence",
  "plural": "Residences",
  "base": "user",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "floor": {
      "type": "string"
    },
    "number": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "CustomAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "community": {
      "type": "belongsTo",
      "model": "Community",
      "foreignKey": ""
    },
    "bucket": {
      "type": "hasOne",
      "model": "Bucket",
      "foreignKey": ""
    },
    "wasteCollections": {
      "type": "hasMany",
      "model": "WasteCollection",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "authenticated",
      "permission": "ALLOW",
      "property": [
        "logout",
        "changePassword"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "__get__community",
        "__get__accessTokens",
        "__findById__buckets",
        "__get__buckets",
        "lastWasteCollection",
        "dateCollection"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "create",
        "find"
      ]
    }
  ],
  "methods": {}
}
