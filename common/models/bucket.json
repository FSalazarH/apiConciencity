{
  "name": "Bucket",
  "base": "CustomPersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "capacity": {
      "type": "number"
    },
    "active": {
      "type": "Boolean",
      "default": false
    },
    "created_at": {
      "type": "date",
      "required": false
    },
    "assigned_at": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": { 
    "residence": {
      "type": "belongsTo",
      "model": "Residence",
      "foreignKey": ""
    },
    "wasteCollections": {
      "type": "hasMany",
      "model": "WasteCollection",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "conciencity",
      "permission": "ALLOW",
      "property": [
        "find",
        "create",
        "deleteById",
        "patchAttributes",
        "__get__wasteCollections"
      ]
    }
  ],
  "methods": {}
}
